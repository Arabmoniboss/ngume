                      {{{discountedPrice}}}
                    </span>
                  {{else}}
                    <span class="ajaxcart__price">
	                  {{{price}}}
                    </span>
                  {{/if}}
                </div>
              </div>
              {{#if discountsApplied}}
                <div class="grid grid--full display-table">
                  <div class="grid__item text-right">
                    {{#each discounts}}
                      <small class="ajaxcart__discount">{{this.title}}</small>
                    {{/each}}
